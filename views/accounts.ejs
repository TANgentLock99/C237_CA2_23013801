<html lang="en">
<head>
  <title>Online Electronic Web application</title>
</head>
<body>
    <%- include ('partials/homebar') -%>
    <div class="container">
      <br>
      <div class="text-center"><h2>Products For Sale</h2></div>
      <br>
    
      <table class="table table-hover small text-center">
        <thead>
            <tr>
              <th width="100">User Name</th>
              <th width="100">Image</th>
              <th width="50">Email</th>
            </tr>
        </thead>
        <tbody>
          <% for(let i=0; i < users.length; i++) { %>
            <tr>
              <td><a href = "/index"><%= users[i].userName %></a></td>
              <td><img src = "/images/<%= users[i].image %>" width = "20%"></td>
              <td><%= users[i].email %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
</body>